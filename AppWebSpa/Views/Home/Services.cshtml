@{
    ViewData["Title"] = "Services";
}
<div class="pagetitle">
    <h1>Servicios</h1>
    <nav>
        <ol class="breadcrumb">
            <li class="breadcrumb-item"><a asp-controller="Home" asp-action="Index">Inicio</a></li>
            <li class="breadcrumb-item"><a asp-controller="Categories" asp-action="Index">Categorias</a></li>
            <li class="breadcrumb-item active">Nuevo Servicio</li>
        </ol>
    </nav>
</div><!-- End Page Title -->

<section class="section dashboard">
    <div class="card">
        <div class="card-header">
            @Model.title
        </div>
        <div class="card-body">
               @Html.Raw(@Model.content)
        </div>
    </div>
</section>

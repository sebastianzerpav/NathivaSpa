@model SpaService

@{
    ViewData["Title"] = "Servicios";
   
}

<div class="pagetitle">
    <h1>Inicio</h1>
    <nav>
        <ol class="breadcrumb">
            <li class="breadcrumb-item"><a asp-action="Dashboard">Inicio</a></li>
            <li class="breadcrumb-item"><a asp-action="Category" asp-route-id="@Model.CategoryId">Categoria</a></li>
            <li class="breadcrumb-item active">Servicio</li>
        </ol>
    </nav>
</div><!-- End Page Title -->

<section class="section dashboard">

    <div class="card">
        <div class="card-header">@Model.Name</div>
            <div class="card-body">
                @Html.Raw(@Model.Description)
            </div>
    </div>

</section>

